
<Gizinta xmlns:gizinta="http://gizinta.com" errorTableName="gzError" logTableName="gzLog">
	<Extract>
		<GDBDataset sourceIDField="FACILITYID" sourceName="wNetworkStructure" targetName="wNetworkStructure"/>
	</Extract>
	<Transform>
		<Dataset name="wNetworkStructure" qa="CheckFields,CheckGeometry" sourceIDField="FACILITYID" sourceNameField="FACILITYID">
			<Field>
				<SourceName qa="Optional">FACILITYID</SourceName>
				<TargetName qa="Optional">FACILITYID</TargetName>
				<Method>Copy</Method>
				<FieldType>String</FieldType>
				<FieldLength>20</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional">INSTALLDATE</SourceName>
				<TargetName qa="Optional">INSTALLDATE</TargetName>
				<Method>Copy</Method>
				<FieldType>Date</FieldType>
				<FieldLength>8</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional">LOCDESC</SourceName>
				<TargetName qa="Optional">LOCDESC</TargetName>
				<Method>Copy</Method>
				<FieldType>String</FieldType>
				<FieldLength>200</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional">ROTATION</SourceName>
				<TargetName qa="Optional">ROTATION</TargetName>
				<Method>Copy</Method>
				<FieldType>Double</FieldType>
				<FieldLength>8</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional">NAME</SourceName>
				<TargetName qa="Optional">NAME</TargetName>
				<Method>Copy</Method>
				<FieldType>String</FieldType>
				<FieldLength>20</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional"/>
				<TargetName qa="Optional">OPDATE</TargetName>
				<Method>None</Method>
				<FieldType>Date</FieldType>
				<FieldLength>8</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional">STRUCTTYPE</SourceName>
				<TargetName qa="Optional">STRUCTTYPE</TargetName>
				<Method>Copy</Method>
				<FieldType>String</FieldType>
				<FieldLength>30</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional"/>
				<TargetName qa="Optional">ENABLED</TargetName>
				<Method>DefaultValue</Method>
				<DefaultValue>1</DefaultValue>

				<FieldType>SmallInteger</FieldType>
				<FieldLength>2</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional"/>
				<TargetName qa="Optional">ACTIVEFLAG</TargetName>
				<Method>DefaultValue</Method>
				<DefaultValue>1</DefaultValue>
				<FieldType>SmallInteger</FieldType>
				<FieldLength>2</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional"/>
				<TargetName qa="Optional">OWNEDBY</TargetName>
				<Method>DefaultValue</Method>
				<DefaultValue>1</DefaultValue>
				<FieldType>SmallInteger</FieldType>
				<FieldLength>2</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional"/>
				<TargetName qa="Optional">MAINTBY</TargetName>
				<Method>DefaultValue</Method>
				<DefaultValue>1</DefaultValue>
				<FieldType>SmallInteger</FieldType>
				<FieldLength>2</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional">LASTUPDATE</SourceName>
				<TargetName qa="Required">LASTUPDATE</TargetName>
				<Method>PythonCalculate</Method>
				<PythonExpression>gzSupport.getDBTime()</PythonExpression>
				<FieldType>Date</FieldType>
				<FieldLength>8</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional">LASTEDITOR</SourceName>
				<TargetName qa="Required">LASTEDITOR</TargetName>
				<Method>PythonCalculate</Method>
				<PythonExpression>myGizinta.defaultUserName()</PythonExpression>
				<FieldType>String</FieldType>
				<FieldLength>50</FieldLength>
			</Field>
			<Field>
				<SourceName qa="Optional"/>
				<TargetName qa="Optional">AncillaryRole</TargetName>
				<Method>None</Method>
				<FieldType>SmallInteger</FieldType>
				<FieldLength>2</FieldLength>
			</Field>
			<SourceFieldNames>FACILITYID,INSTALLDATE,LASTEDITOR,LASTUPDATE,LOCDESC,MAINTBY,NAME,OWNEDBY,ROTATION,STRUCTTYPE</SourceFieldNames>
		</Dataset>
	</Transform>
</Gizinta>